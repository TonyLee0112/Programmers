def solution(book_time):
    for i in range(len(book_time)):
        for j in range(2):
            a = list(map(int, book_time[i][j].split(":")))
            book_time[i][j] = 60 * a[0] + a[1]
            if j == 1:
                book_time[i][j] += 10
    book_time.sort()
    current_time = book_time[0][0]
    len_max = 0
    q = [book_time.pop(0)]
    while q or book_time:

        # 시작시간되면 book_time 에서 꺼내서 q에 넣기
        if book_time:
            for room in book_time:
                start = room[0]
                if start <= current_time:
                    q.append(book_time.pop(book_time.index(room)))

        # 종료시간되면 q에서 꺼내기
        if q:
            for room in q:
                end = room[1]
                if end <= current_time:
                    q.pop(q.index(room))

        # max(len(q)) 찾기
        if len_max < len(q):
            len_max = len(q)

        # 현재 시간 갱신
        current_time += 1
    return len_max
